package MONEYCONTROL.com.moneycontrol;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.chrome.ChromeOptions;
import org.openqa.selenium.firefox.FirefoxDriver;

public class SeleniumC {
WebDriver driver;

public void browsersetUp()
{
	String path=System.getProperty("user.dir");
	System.setProperty("Webdriver.gecko.driver",path+ "//resources//geckodriver.exe");
	/*
	 * ChromeOptions chromeOptions = new ChromeOptions();
	 * chromeOptions.addArguments("--whitelist-ip *");
	 * chromeOptions.addArguments("--proxy-server='direct://'");
	 * chromeOptions.addArguments("--proxy-bypass-list=*"); driver=new
	 * ChromeDriver(chromeOptions);
	 */
	driver=new FirefoxDriver();
	driver.get("naukri.com");
}
}
