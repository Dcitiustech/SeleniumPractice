package com.seleniumconcepts.SELENIUMCONCEPT;

import java.util.List;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.testng.annotations.Test;

public class DynamicWebtable extends BaseTestClass{
	
	@Test
    public void findMaxValueinWebtable()
    {
       WebElement table= driver.findElement(By.className("dataTable"));
     List<WebElement> lst_rows=  table.findElements(By.xpath("//tr"));
     int size=lst_rows.size();
     System.out.println(size);
    }

}
